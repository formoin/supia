{
  "expo": {
    "name": "supia_app",
    "slug": "supia_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      }
    },
    "web": {}
  },

  "@react-native-kakao/core": {
    "nativeAppKey": "{{native app key}}",
    "android": {
      "authCodeHandlerActivity": true
    },
    "ios": {
      "handleKakaoOpenUrl": true
    }
  }
}
